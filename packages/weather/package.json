    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "rollup": "^4.36.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.0"
  },
  "prettier": "@junat/prettier",
  "dependencies": {
    "fast-xml-parser": "^5.0.9"
