{
  "name": "@junat/graphql",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "concurrently \"tsc --watch --preserveWatchOutput\" \"gql-gen -w\" \"rollup -cw\"",
    "build": "pnpm tsup && tsc --build",
    "gql-gen": "graphql-codegen",
    "check-types": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "eslint ."
  },
  "imports": {
    "#generated/digitraffic": "./src/digitraffic/generated/index.ts",
    "#generated/digitraffic/*": "./src/digitraffic/generated/*",
    "#generated/digitransit": "./src/digitransit/generated/index.ts",
    "#generated/digitransit/*": "./src/digitransit/generated/*",
    "#fragments/*": "./src/fragments/*",
    "#queries/*": "./src/queries/*",
    "#*": "./src/*"
  },
  "exports": {
    "./graphql-request": {
      "types": "./dist/graphql_request.d.ts",
      "default": "./dist/graphql_request.js"
    },
    "./digitraffic/queries/*": {
      "types": "./dist/digitraffic/queries/*.d.ts",
      "default": "./dist/digitraffic/queries/*.js"
    },
    "./digitraffic": {
      "types": "./dist/generated/digitraffic/graphql.d.ts",
      "default": "./dist/generated/digitraffic/graphql.js"
    },
    "./digitransit/queries/*": {
      "types": "./dist/digitransit/queries/*.d.ts",
      "default": "./dist/digitransit/queries/*.js"
    },
    "./digitransit": {
      "types": "./dist/generated/digitransit/graphql.d.ts",
      "default": "./dist/generated/digitransit/graphql.js"
    }
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@junat/core": "workspace:^",
    "@junat/eslint": "workspace:*",
    "@junat/prettier": "workspace:*",
    "@junat/rollup": "workspace:^",
    "@junat/tsconfig": "workspace:^",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.16.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "rollup": "^4.21.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "prettier": "@junat/prettier",
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@junat/digitraffic": "workspace:^",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0"
  }
}
