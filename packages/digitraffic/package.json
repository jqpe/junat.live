{
  "name": "@junat/digitraffic",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jqpe/junat.live/issues"
  },
  "homepage": "https://github.com/jqpe/junat.live",
  "description": "Utilities for working with Digitraffic's RESTful APIs.",
  "author": "Jasper Nyk√§nen <nykanen@hey.com> (https://github.com/jqpe)",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./handlers/*": {
      "types": "./dist/handlers/*.d.ts",
      "default": "./dist/handlers/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "shx rm -rf lib && tsc --build && rollup -c",
    "test": "vitest run",
    "check-types": "tsc --noEmit --emitDeclarationOnly false",
    "coverage": "vitest run --coverage",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@junat/prettier": "workspace:*",
    "@junat/rollup": "workspace:^",
    "@junat/tsconfig": "workspace:^",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "20.12.11",
    "@vitest/ui": "1.6.0",
    "c8": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-config-junat.live": "workspace:*",
    "json": "^11.0.0",
    "msw": "2.2.14",
    "node-fetch": "^3.3.0",
    "rollup": "^4.18.0",
    "shx": "^0.3.4",
    "tslib": "^2.6.3",
    "typescript": "5.4.5",
    "vite": "^4.4.9",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "core-js": "3.34.0"
  },
  "prettier": "@junat/prettier"
}
