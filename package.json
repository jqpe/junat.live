{
  "name": "junat.live",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:digitraffic": "yarn --cwd=packages/digitraffic generate:fixtures",
    "build:digitraffic-mqtt": "yarn --cwd=packages/digitraffic-mqtt build",
    "build:site": "yarn --cwd=site build",
    "build": "yarn build:digitraffic && yarn build:digitraffic-mqtt && yarn build:site",
    "dev": "yarn --cwd site run dev",
    "test": "yarn vitest --run"
  },
  "workspaces": [
    "packages/*",
    "site"
  ]
}
