{
  "all": true,
  "reporter": ["lcov"],
  "exclude": [
    "**/.next",
    "**/dist",
    "**/types",
    "**/coverage",
    "**/.storybook/",
    "**/mocks",
    "**/constants/",
    "packages/*/test{,s}/**",
    "packages/eslint-config-junat.live",
    "packages/stitches-config",
    "**/*.d.ts",
    "test{,s}/**",
    "test{,-*}.{js,cjs,mjs,ts,tsx,jsx}",
    "**/*{.,-}test.{js,cjs,mjs,ts,tsx,jsx}",
    "**/*stories.{mdx,js,ts,jsx,tsx}",
    "**/{vite,vitest,next,ecosystem,stitches}.config.{js,ts}",
    "**/.{eslint,mocha}rc.{js,cjs}"
  ]
}
